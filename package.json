{
    "name": "json-parse-validator",
    "displayName": "json-parse-validator",
    "description": "Uses the JSON.parse() method to validate a JSON document.",
    "repository": {
        "type": "git",
        "url": "https://github.com/AdrianSanguineti/json-parse-validator.git"
    },
    "publisher": "AdrianSanguineti",
    "version": "1.1.1",
    "license": "MIT",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Linters"
    ],
    "activationEvents": [
        "onLanguage:json",
        "onCommand:extension.json-parse-validator"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "extension.json-parse-validator",
            "title": "Parse with JSON.parse()"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.36.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vscode-nls-dev": "^3.3.1",
        "vscode-test": "^1.0.0-next.0"
    },
    "dependencies": {
        "vscode-nls": "^4.1.1"
    },
    "icon": "./images/logo.png"
}